{
  "name": "ship-components-video",
  "version": "0.1.0",
  "description": "React video component using HTML5 video element",
  "main": "src/root.js",
  "scripts": {
    "start": "grunt server",
    "lint": "./node_modules/eslint/bin/eslint.js src/**/*.js",
    "test": "jest",
    "test:watchAll": "npm run test -- --watchAll --coverage=false",
    "watch:test": "jest --coverage --watch --coverage=false",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js -v",
    "build": "npm test && grunt clean && grunt build",
    "generate-docs": "node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ship-components/ship-components-video.git"
  },
  "engines": {
    "node": ">=4.0 <9.0"
  },
  "keywords": [
    "react",
    "es2015",
    "es2016",
    "es2017",
    "javascript",
    "javascript-library",
    "grunt",
    "webpack",
    "jest"
  ],
  "author": "Sepand Assadi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ship-components/ship-components-video/issues"
  },
  "homepage": "git+https://github.com/ship-components/ship-components-video#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "dashjs": "^2.6.7",
    "docdash": "^0.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-config-ship-components": "^1.5.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "flux": "^3.1.3",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-coveralls": "^1.0.1",
    "grunt-eslint": "^20.0.0",
    "grunt-webpack": "^3.0.0",
    "immutable": "^3.8.2",
    "invariant": "^2.2.4",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "jest-junit": "^3.1.0",
    "jsdoc": "^3.5.5",
    "package-banner": "^0.1.1",
    "postcss-calc": "^5.3.1",
    "postcss-color-function": "^4.0.0",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.0.2",
    "postcss-simple-vars": "^4.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.1",
    "ship-components-checkbox": "^0.2.0",
    "ship-components-icon": "^0.1.2",
    "ship-components-select": "^1.1.1",
    "ship-components-subscribe": "^0.2.0",
    "ship-components-textinput": "^0.3.9",
    "style-loader": "^0.18.2",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "testResultsProcessor": "jest-junit",
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.css$": "<rootDir>/node_modules/jest-css-modules"
    },
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!ship-components-*)"
    ],
    "testMatch": [
      "**/__tests__/**/*-test.js?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "__tests__/"
    ]
  },
  "jest-junit": {
    "suiteName": "Client Tests",
    "output": "./test-reports/junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "babelJest": {
    "include": [
      "src/**/*.@(js|jsx|es6)",
      "<rootDir>/node_modules"
    ]
  }
}
